@model Clientes
@{
    ViewData["Title"] = "GuardarForm";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>GuardarForm</h1>
<form>
    <div class="mb-3">
        <label class="form-label">Codigo cliente</label>
        <input asp-for="CLIENTES_COD" type="number" class="form-control" />
        <span asp-validation-for="CLIENTES_COD" class="text-denger"></span>
    </div>
    <div class="mb-3">
        <label class="form-label">Correo</label>
        <input asp-for="CORREO" type="text" class="form-control" />
        <span asp-validation-for="CORREO" class="text-denger"></span>
    </div>
    <div class="mb-3">
        <label class="form-label">Contraseña</label>
        <input asp-for="CONTRASENIA" type="password" class="form-control" />
        <span asp-validation-for="CONTRASENIA" class="text-denger"></span>
    </div>
    <div class="mb-3">
        <label class="form-label">Tipo de cliente</label>
        <input asp-for="TIPO_CLIENTE" type="s" class="form-control" />
        <span asp-validation-for="TIPO_CLIENTE" class="text-denger"></span>
    </div>
    <div class="mb-3">
        <label class="form-label">Razon social</label>
        <input asp-for="RAZON_SOCIAL" type="text" class="form-control" />
        <span asp-validation-for="RAZON_SOCIAL" class="text-denger"></span>
    </div>
    <div class="mb-3">
        <label class="form-label">CUIT</label>
        <input asp-for="CUIT_DNI" type="number" class="form-control" />
        <span asp-validation-for="CUIT_DNI" class="text-denger"></span>
    </div>
    <div class="mb-3">
        <label class="form-label">Nombre</label>
        <input asp-for="NOMBRE" type="text" class="form-control" />
        <span asp-validation-for="NOMBRE" class="text-denger"></span>
    </div>
    <div class="mb-3">
        <label class="form-label">Apellido</label>
        <input asp-for="APELLIDO" type="text" class="form-control" />
        <span asp-validation-for="APELLIDO" class="text-denger"></span>
    </div>
    <div class="mb-3">
        <label class="form-label">Codigo usuarios</label>
        <input asp-for="USUARIOS_CODIGO" type="number" class="form-control" />
        <span asp-validation-for="USUARIOS_CODIGO" class="text-denger"></span>
    </div>
    <button type="submit" class="btn btn-primary">Guardar</button>
    <a asp-action="Listar" asp-controller="Cliente" class="btn btn-warning">Volver al listado</a>
</form>
